# Program for LC3 TLM model with interrupt
# Written by snehiths@vayavyalabs.com

#Interrupt vector

A 0x0000
D 0x000F
D 0x0E1E

# Reset handler

A 0x0010
#Set stack pointer
D 0xEAEF 
#Jump to 0x3000 using RTI
D 0x2003
D 0x30ED
D 0x1B61
D 0x8000
D 0x3000

# Interrupt handler
A 0x0020
D 0x8000

#main
A 0x3000
D 0x36AA
D 0x78D5
D 0x163F
